<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp">
  <display-name>BankonetWeb</display-name>
  <servlet>
    <servlet-name>DateServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.DateServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DateServlet</servlet-name>
    <url-pattern>/date</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TraiterLoginServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.TraiterLoginServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>TraiterLoginServlet</servlet-name>
    <url-pattern>/traiterLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeconnectionServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.DeconnectionServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeconnectionServlet</servlet-name>
    <url-pattern>/fin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CompteCourantServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.CompteCourantServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompteCourantServlet</servlet-name>
    <url-pattern>/compteCourantListe</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CompteEpargneServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.CompteEpargneServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompteEpargneServlet</servlet-name>
    <url-pattern>/compteEpargneListe</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DetailCompteEpargneServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.DetailCompteEpargneServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailCompteEpargneServlet</servlet-name>
    <url-pattern>/detailCompteEpargne</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InitVirementServlet</servlet-name>
    <servlet-class>com.bankonet.servlet.InitVirementServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitVirementServlet</servlet-name>
    <url-pattern>/initVirement</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/erreur.jsp</location>
  </error-page>
  <filter>
  	<filter-name>servletTimeFilter</filter-name>
  	<filter-class>com.bankonet.filter.servletTimeFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>servletTimeFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>